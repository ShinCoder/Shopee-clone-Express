{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended","airbnb-base", "prettier"],
    "overrides": [
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": [
            "warn",
            {
                "endOfLine": "auto"
            }
        ],
        "import/extensions": "off",
        "no-unused-vars": "warn",
        "import/prefer-default-export": "warn"
    }
}
